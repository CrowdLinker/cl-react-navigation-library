{
  "name": "react-navigation-library",
  "version": "0.1.1",
  "description": "navigation library for react native",
  "main": "dist/index.js",
  "umd:main": "dist/react-navigation-library.umd.production.js",
  "module": "dist/react-navigation-library.es.production.js",
  "typings": "dist/index.d.ts",
  "keywords": [
    "react",
    "react-native",
    "navigation",
    "react-navigation",
    "react-native-navigation"
  ],
  "author": "andrew smith",
  "repository": {
    "type": "git",
    "url": "https://github.com/andyboythekid/react-navigation-library"
  },
  "license": "ISC",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --env=jsdom"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-native": ">=0.51",
    "react-native-gesture-handler": "*"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "jest": {
    "preset": "react-native",
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|react-native-gesture-handler)/)"
    ],
    "transform": {
      "\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@types/jest": "^24.0.15",
    "@types/react": "^16.8.20",
    "@types/react-dom": "^16.8.4",
    "@types/react-native": "^0.57.57",
    "husky": "^2.3.0",
    "metro-react-native-babel-preset": "^0.54.1",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-native": "^0.59.8",
    "react-native-gesture-handler": "^1.3.0",
    "react-native-testing-library": "^1.7.0",
    "react-test-renderer": "^16.8.6",
    "tsdx": "^0.6.1",
    "tslib": "^1.10.0",
    "typescript": "^3.5.2"
  }
}
